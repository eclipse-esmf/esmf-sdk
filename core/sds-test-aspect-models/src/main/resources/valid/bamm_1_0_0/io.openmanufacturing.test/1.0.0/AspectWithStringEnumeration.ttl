@prefix : <urn:bamm:io.openmanufacturing.test:1.0.0#> .
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:1.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:1.0.0#> .
@prefix bamm-e: <urn:bamm:io.openmanufacturing:entity:1.0.0#> .
@prefix unit: <urn:bamm:io.openmanufacturing:unit:1.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:AspectWithEnumeration a bamm:Aspect ;
   bamm:name "AspectWithEnumeration" ;
   bamm:properties ( :enumerationProperty ) ;
   bamm:operations ( ) .

:enumerationProperty a bamm:Property ;
   bamm:name "enumerationProperty" ;
   bamm:preferredName ""@en ;
   bamm:description ""@en ;
   bamm:characteristic [
     a bamm-c:Enumeration ;
     bamm:name "Foo" ;
     bamm:dataType xsd:string ;
     bamm-c:values ( "foo" "bar" )
   ] .

