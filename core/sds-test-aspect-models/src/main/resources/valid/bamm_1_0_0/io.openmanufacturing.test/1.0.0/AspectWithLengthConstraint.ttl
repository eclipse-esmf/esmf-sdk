@prefix : <urn:bamm:io.openmanufacturing.test:1.0.0#> .
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:1.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:1.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix unit: <urn:bamm:io.openmanufacturing:unit:1.0.0#> .

:AspectWithLengthConstraint a bamm:Aspect ;
   bamm:name "AspectWithLengthConstraint" ;
   bamm:preferredName "Test Aspect"@en ;
   bamm:description "This is a test description"@en ;
   bamm:see <http://example.com/omp> ;
   bamm:properties ( :testProperty ) ;
   bamm:operations ( ) .

:testProperty a bamm:Property ;
   bamm:name "testProperty" ;
   bamm:preferredName "Test Property"@en ;
   bamm:description "This is a test property."@en ;
   bamm:see <http://example.com/omp> ;
   bamm:see <http://example.com/me> ;
   bamm:exampleValue "Test123" ;
   bamm:characteristic :TestLengthConstraint .

:TestLengthConstraint a bamm-c:Trait ;
   bamm:name "TestLengthConstraint" ;
   bamm-c:constraint [
      a bamm-c:LengthConstraint ;
      bamm:preferredName "Test Length Constraint"@en ;
      bamm:description "This is a test length constraint."@en ;
      bamm:see <http://example.com/omp> ;
      bamm-c:minValue "5"^^xsd:nonNegativeInteger ;
      bamm-c:maxValue "10"^^xsd:nonNegativeInteger ;
   ] ;
   bamm-c:baseCharacteristic bamm-c:Text .

