@prefix : <urn:bamm:io.openmanufacturing.test:1.0.0#> .
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:1.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:1.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix unit: <urn:bamm:io.openmanufacturing:unit:1.0.0#> .

:AspectWithListAndAdditionalProperty a bamm:Aspect ;
   bamm:name "AspectWithListAndAdditionalProperty" ;
   bamm:preferredName "Test Aspect"@en ;
   bamm:description "This is a test description"@en ;
   bamm:see <http://example.com/omp> ;
   bamm:properties ( :testProperty :testPropertyTwo ) ;
   bamm:operations ( ) .

:testProperty a bamm:Property ;
   bamm:name "testProperty" ;
   bamm:preferredName "Test Property"@en ;
   bamm:description "This is a test property."@en ;
   bamm:see <http://example.com/omp> ;
   bamm:see <http://example.com/me> ;
   bamm:exampleValue "Example Value" ;
   bamm:characteristic :TestList .

:testPropertyTwo a bamm:Property ;
   bamm:name "testPropertyTwo" ;
   bamm:preferredName "Test Property Two"@en ;
   bamm:description "This is a test property."@en ;
   bamm:see <http://example.com/omp> ;
   bamm:see <http://example.com/me> ;
   bamm:exampleValue "Example Value" ;
   bamm:characteristic bamm-c:Text .

:TestList a bamm-c:List ;
   bamm:name "TestList" ;
   bamm:preferredName "Test List"@en ;
   bamm:description "This is a test list."@en ;
   bamm:see <http://example.com/omp> ;
   bamm:dataType xsd:string .
