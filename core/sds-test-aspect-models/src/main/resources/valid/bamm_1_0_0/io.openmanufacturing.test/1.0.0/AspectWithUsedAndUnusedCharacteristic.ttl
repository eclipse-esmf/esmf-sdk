@prefix : <urn:bamm:io.openmanufacturing.test:1.0.0#> .
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:1.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:1.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:AspectWithUsedAndUnusedCharacteristic a bamm:Aspect ;
   bamm:name "AspectWithUsedAndUnusedCharacteristic" ;
   bamm:properties ( :testProperty ) ;
   bamm:operations ( ) .

:testProperty a bamm:Property ;
   bamm:name "testProperty" ;
   bamm:characteristic :UsedTestCharacteristic .

:UsedTestCharacteristic a bamm:Characteristic ;
   bamm:name "UsedTestCharacteristic" ;
   bamm:preferredName "Used Test Characteristic"@en ;
   bamm:description "Used Test Characteristic"@en ;
   bamm:see <http://example.com/omp> ;
   bamm:see <http://example.com/me> ;
   bamm:dataType xsd:string .

:UnusedTestCharacteristic a bamm:Characteristic ;
   bamm:name "UnusedTestCharacteristic" ;
   bamm:preferredName "Unused Test Characteristic"@en ;
   bamm:description "Unused Test Characteristic"@en ;
   bamm:see <http://example.com/omp> ;
   bamm:see <http://example.com/me> ;
   bamm:dataType xsd:string .
