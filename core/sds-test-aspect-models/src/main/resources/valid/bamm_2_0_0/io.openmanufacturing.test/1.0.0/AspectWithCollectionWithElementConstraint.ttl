@prefix : <urn:bamm:io.openmanufacturing.test:1.0.0#> .
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:2.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:2.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix unit: <urn:bamm:io.openmanufacturing:unit:2.0.0#> .

:AspectWithCollectionWithElementConstraint a bamm:Aspect ;
   bamm:name "AspectWithCollectionWithElementConstraint" ;
   bamm:preferredName "Test Aspect"@en ;
   bamm:description "This is a test description"@en ;
   bamm:see <http://example.com/omp> ;
   bamm:properties ( :testProperty ) ;
   bamm:operations ( ) .

:testProperty a bamm:Property ;
   bamm:name "testProperty" ;
   bamm:preferredName "Test Property"@en ;
   bamm:description "This is a test property."@en ;
   bamm:see <http://example.com/omp> ;
   bamm:see <http://example.com/me> ;
   bamm:exampleValue "5.0"^^xsd:float ;
   bamm:characteristic :TestCollection .

:TestCollection a bamm-c:Collection ;
   bamm:name "TestCollection" ;
   bamm:preferredName "Test Collection"@en ;
   bamm:description "This is a test collection."@en ;
   bamm:see <http://example.com/omp> ;
   bamm-c:elementCharacteristic [
      a bamm-c:Trait ;
      bamm:name "ElementRangeConstraint" ;
      bamm-c:baseCharacteristic :Measurement ;
      bamm-c:constraint [
         a bamm-c:RangeConstraint ;
         bamm-c:minValue "2.3"^^xsd:float ;
         bamm-c:lowerBoundDefinition bamm-c:AT_LEAST ;
         bamm-c:maxValue "10.5"^^xsd:float ;
         bamm-c:upperBoundDefinition bamm-c:AT_MOST ;
      ]
   ] .

:Measurement a bamm-c:Measurement ;
   bamm:name "Measurement" ;
   bamm:dataType xsd:float ;
   bamm-c:unit unit:metrePerSecond .
