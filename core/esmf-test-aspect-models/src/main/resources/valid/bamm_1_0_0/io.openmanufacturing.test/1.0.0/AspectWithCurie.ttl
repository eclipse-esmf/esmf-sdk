@prefix : <urn:samm:org.eclipse.esmf.samm.test:1.0.0#> .
@prefix bamm: <urn:samm:org.eclipse.esmf.samm:meta-model:1.0.0#> .
@prefix bamm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:1.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:AspectWithCurie a bamm:Aspect ;
   bamm:name "AspectWithCurie" ;
   bamm:properties ( :testCurie :testCurieWithoutExampleValue ) ;
   bamm:operations ( ) .

:testCurie a bamm:Property ;
   bamm:name "testCurie" ;
   bamm:exampleValue "unit:hectopascal"^^bamm:curie ;
   bamm:characteristic bamm-c:UnitReference .

:testCurieWithoutExampleValue a bamm:Property ;
   bamm:name "testCurieWithoutExampleValue" ;
   bamm:characteristic bamm-c:UnitReference .
