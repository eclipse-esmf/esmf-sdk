@prefix : <urn:samm:org.eclipse.esmf.samm.test:1.0.0#> .
@prefix bamm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.0.0#> .
@prefix bamm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:AspectWithConstraintWithSeeAttribute a bamm:Aspect ;
   bamm:properties ( :testProperty :testPropertyTwo ) ;
   bamm:operations ( ) .

:testProperty a bamm:Property ;
   bamm:characteristic :TestTrait .

:testPropertyTwo a bamm:Property ;
   bamm:characteristic :TestTraitTwo .

:TestTrait a bamm-c:Trait ;
   bamm-c:constraint [
      a bamm-c:LengthConstraint ;
      bamm:preferredName "Test Constraint"@en ;
      bamm:description "Test Constraint"@en ;
      bamm:see <http://example.com/omp> ;
      bamm-c:minValue "5"^^xsd:nonNegativeInteger ;
      bamm-c:maxValue "10"^^xsd:nonNegativeInteger ;
   ] ;
   bamm-c:baseCharacteristic bamm-c:Text .

:TestTraitTwo a bamm-c:Trait ;
   bamm-c:constraint [
      a bamm-c:LengthConstraint ;
      bamm:preferredName "Test Constraint Two"@en ;
      bamm:description "Test Constraint Two"@en ;
      bamm:see <http://example.com/omp> ;
      bamm-c:minValue "5"^^xsd:nonNegativeInteger ;
      bamm-c:maxValue "10"^^xsd:nonNegativeInteger ;
   ] ;
   bamm-c:baseCharacteristic bamm-c:Text .
