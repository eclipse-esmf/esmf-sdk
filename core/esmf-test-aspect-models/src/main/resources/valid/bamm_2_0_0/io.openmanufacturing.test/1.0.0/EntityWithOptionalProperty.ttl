@prefix : <urn:samm:org.eclipse.esmf.samm.test:1.0.0#> .
@prefix bamm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.0.0#> .
@prefix bamm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:EntityWithOptionalProperty a bamm:Entity ;
   bamm:preferredName "Test Entity"@en ;
   bamm:description "This is a test entity."@en ;
   bamm:see <http://example.com/omp> ;
   bamm:properties ( [ bamm:property :testProperty ; bamm:optional "true"^^xsd:boolean ] ) .

:testProperty a bamm:Property ;
   bamm:characteristic bamm-c:Text .
